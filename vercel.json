{
    "buildCommand":"npm i && npm run build",
    "outputDirectory":"dist",
    "functions": {
        "api/index.js": {
          "memory": 1024,
          "includeFiles": "./api/data/db.json"
        }
      },
      "rewrites": [
        {
          "source": "/(.*)",
          "destination": "api/index.js"
        }
      ]
}